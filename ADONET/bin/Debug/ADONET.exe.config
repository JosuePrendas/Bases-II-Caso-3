<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
  <appSettings>
    <add key="Pooling" value="Data Source=localhost\MSSQLSERVER01; Initial Catalog=bd;Integrated Security=true; Max Pool Size=10; Min Pool Size=10;"/>
    <add key="NoPooling" value="Data Source=localhost\MSSQLSERVER01; Initial Catalog=bd;Integrated Security=true; Pooling=false;"/>
    <caching>
      <sqlCacheDependency pollTime="2000" enabled="true">
        <databases>
          <add name="Caching" connectionStringName="Pooling"/>
        </databases>
      </sqlCacheDependency>
    </caching>
  </appSettings>
</configuration>